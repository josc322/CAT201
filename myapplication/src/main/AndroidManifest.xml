<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.airline.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name=".NotificationActivity"/>

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FinishActivity"/>
        <activity android:name=".PayableActivity" />
        <activity android:name=".CreditActivity" />
        <activity android:name=".PointActivity" />
        <activity android:name=".PayActivity" />
        <activity android:name=".ConfirmActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".LocationActivity" />
        <activity android:name=".SeatActivity" />
        <activity android:name=".LoginActivity"/>
        <activity android:name=".AirlineActivity" />
        <activity
            android:name=".OfferActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".EventActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".ContactActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".HelpActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".DetailActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".CancelActivity"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".SettingActivity"
            android:parentActivityName=".NavigationActivity" />
    </application>

</manifest>
